<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<!-- $Id: web.xml,v 1.2 2007/07/16 06:11:13 scott Exp $ -->
<web-app>
	<servlet>
		<servlet-name>lsn</servlet-name>
		<servlet-class>lsn.servlet.ProcessMessage</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>prov</servlet-name>
		<servlet-class>lsn.servlet.Provision</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>lsn</servlet-name>
		<url-pattern>/lsn</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>prov</servlet-name>
		<url-pattern>/prov</url-pattern>
	</servlet-mapping>
</web-app>
